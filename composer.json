{
    "name": "wisedragonstd/hades-wrapper",
    "description": "Hades is a lightweight and high-extensible wrapper for Telegram Bots' APIs.",
    "version": "0.3.6",
    "type": "library",
    "license": "LGPL-3.0+",
    "keywords": ["telegram", "bot", "wrapper", "telegram-bot"],
    "homepage": "https://gitlab.com/WiseDragonStd/HadesWrapper",
    "authors": [
        {
            "name": "Dany Spin",
            "homepage": "https://gitlab.com/u/danyspin97",
            "role": "Lead Developer"
        },
        {
            "name": "TheSeeker",
            "homepage": "https://gitlab.com/u/dom.theseeker",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-redis": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.4.*"
    },
    "suggest": {
        "ext-mysql": "*",
        "ext-pgsql": "*"
    },
    "autoload": {
        "psr-4": {
            "WiseDragonStd\\HadesWrapper\\": ["lib/hades", "lib/hades-sql"]
        }
    },
    "archive": {
        "exclude": ["/examples", "/templates", "/tests", "/ci", "/*.md", "phpunit.xml"]
    }
}
